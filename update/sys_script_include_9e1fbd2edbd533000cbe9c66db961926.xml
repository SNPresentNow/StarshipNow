        <name>ShipUtils</name>
        <script><![CDATA[var ShipUtils = Class.create();
ShipUtils.prototype = {
    initialize: function() {
    },
	launchShip: function(ship){
		
		
		if(ship.getValue('class')=='shuttle'){
			gs.addInfoMessage('Launching shuttle');
			ship.activity_log='Ship launched';
			ship.update();
		} else {
			gs.addInfoMessage('Can only launch shuttles');  
		}
		
	},

    type: 'ShipUtils'
};]]></script>
